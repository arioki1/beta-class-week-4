<h1>List Products</h1>
<a href="add-data">Tambah Data</a>

    <table style="width:100%" border="1">
    <tr>
        <th>id</th>
        <th>name</th>
        <th>price</th>
    </tr>
    {{#each data}}
        <tr>
            <td>{{id}}</td>
            <td>{{name}}</td>
            <td>{{price}}</td>
        </tr>
    {{/each}}
</table>
